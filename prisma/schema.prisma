// prisma/schema.prisma
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}
model Property {
  id              String   @id
  ownerName       String
  ownerPhone      String
  locationLink    String
  province        String
  status          String
  transactionType String
  isNew           Boolean  @default(false)
  propertyType    String?
  createdAt       DateTime @default(now())
}
