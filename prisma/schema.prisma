generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["dataProxy"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id             String   @id @default(uuid())
  ownerName      String
  ownerPhone     String
  propertyType   String?
  agentName      String?
  status         String?
  investor       String?
  estimatedPrice Int?
  approvedPrice  Int?
  locationLink   String?
  mapEmbedLink   String?
  province       String?
  images         Json     @default("[]")
  createdAt      DateTime @default(now())
}
